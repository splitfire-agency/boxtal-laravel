{
    "name": "splitfire-agency/boxtal-laravel",
    "type": "library",
    "description": "Boxtal PHP Laravel Library for Envoimoinscher/Boxtal API v1",
    "homepage": "https://github.com/splitfire-agency/boxtal-laravel",
    "license": "Unlicense",
    "minimum-stability": "dev",
    "keywords": ["laravel", "envoimoinscher", "boxtal", "php", "emc", "class", "api", "library"],
    "authors": [
        {
            "name": "Boxtal Support",
            "email": "api@boxtal.com"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "laravel/framework": "^8.0",
        "ext-curl":  "*",
        "ext-libxml": "*",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "Emc\\Tests\\": "tests/",
            "Emc\\": "src/"
        }
    },
    "laravel": {
        "providers": [
            "Sf\\Boxtal\\Providers\\PayboxServiceProvider"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "orchestra/testbench": "^6.24"
    }
}
